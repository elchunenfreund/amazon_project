<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Vendor Analytics - Amazon Tracker</title>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;900&display=swap" rel="stylesheet">
    <script src="https://cdn.tailwindcss.com"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chartjs-adapter-date-fns"></script>
    <style>
        body { font-family: 'Inter', sans-serif; }
        .report-cell { min-width: 120px; }
        .sticky-col { position: sticky; left: 40px; z-index: 10; background: inherit; }
        .table-container { overflow-x: auto; }
        thead th { position: sticky; top: 0; z-index: 20; }
        thead th.sticky-col { z-index: 30; }
        .loading-spinner {
            border: 3px solid #f3f3f3;
            border-top: 3px solid #3498db;
            border-radius: 50%;
            width: 20px;
            height: 20px;
            animation: spin 1s linear infinite;
            display: inline-block;
        }
        @keyframes spin {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
        }
        /* Metric display - label directly above value */
        .metric-item { text-align: center; padding: 3px 0; }
        .metric-label { font-size: 0.7rem; color: #64748b; font-weight: 500; line-height: 1; margin-bottom: 1px; }
        .metric-value { font-size: 1rem; font-weight: 700; line-height: 1.2; }
        .metric-group { display: flex; gap: 12px; justify-content: center; flex-wrap: wrap; }
        /* Status badges */
        .unavailable-row { background-color: #fef2f2 !important; }
        .unavailable-row:hover { background-color: #fee2e2 !important; }
        .status-badge { font-size: 0.8rem; padding: 4px 10px; border-radius: 6px; font-weight: 600; display: inline-block; }
        .status-instock { background: #dcfce7; color: #166534; }
        .status-backorder { background: #fef3c7; color: #92400e; }
        .status-unavailable { background: #fee2e2; color: #991b1b; }
        .status-nodata { background: #f1f5f9; color: #64748b; }
        /* Price display */
        .price-display { font-size: 1rem; font-weight: 700; color: #334155; margin-top: 4px; }
        /* Responsive adjustments */
        @media (max-width: 1200px) {
            .report-cell { min-width: 100px; }
            .metric-value { font-size: 0.9rem; }
            .metric-group { gap: 8px; }
        }
        @media (max-width: 768px) {
            .report-cell { min-width: 90px; padding: 6px !important; }
            .metric-value { font-size: 0.85rem; }
            .metric-label { font-size: 0.6rem; }
            .status-badge { font-size: 0.7rem; padding: 3px 6px; }
            thead th { font-size: 0.75rem; padding: 8px 4px !important; }
        }
    </style>
</head>
<body class="bg-slate-100 min-h-screen">
    <div class="max-w-full mx-auto px-4 md:px-8 py-8">
        <!-- Header -->
        <header class="mb-8">
            <div class="flex flex-col md:flex-row justify-between items-start md:items-center gap-4">
                <div>
                    <h1 class="text-4xl font-black text-slate-900 tracking-tight">Vendor Analytics</h1>
                    <p class="text-slate-500 font-medium">ASIN-level performance metrics from SP-API reports</p>
                </div>
                <div class="flex gap-3">
                    <a href="/" class="bg-slate-600 text-white px-6 py-3 rounded-xl font-bold shadow-lg hover:bg-slate-700 transition-all">Dashboard</a>
                    <a href="/purchase-orders" class="bg-purple-600 text-white px-6 py-3 rounded-xl font-bold shadow-lg hover:bg-purple-700 transition-all">Purchase Orders</a>
                </div>
            </div>
        </header>

        <!-- Date Range Filter -->
        <div class="bg-white rounded-2xl shadow-lg p-6 mb-8 border border-slate-200">
            <div class="flex flex-wrap items-end gap-4">
                <div>
                    <label class="block text-sm font-medium text-slate-700 mb-1">Start Date</label>
                    <input type="date" id="startDate" value="<%= startDate %>"
                           class="px-4 py-2 rounded-lg border border-slate-300 focus:ring-2 focus:ring-blue-500 focus:border-blue-500 outline-none">
                </div>
                <div>
                    <label class="block text-sm font-medium text-slate-700 mb-1">End Date</label>
                    <input type="date" id="endDate" value="<%= endDate %>"
                           class="px-4 py-2 rounded-lg border border-slate-300 focus:ring-2 focus:ring-blue-500 focus:border-blue-500 outline-none">
                </div>
                <button onclick="applyDateFilter()" class="bg-blue-600 text-white px-6 py-2 rounded-lg font-bold hover:bg-blue-700 transition-all">
                    Apply Filter
                </button>
                <button onclick="syncSelectedAsins()" id="syncSelectedBtn" class="bg-purple-600 text-white px-6 py-2 rounded-lg font-bold hover:bg-purple-700 transition-all disabled:opacity-50 disabled:cursor-not-allowed" disabled>
                    Sync Selected
                </button>
                <button onclick="refreshAllReports()" id="refreshBtn" class="bg-emerald-600 text-white px-6 py-2 rounded-lg font-bold hover:bg-emerald-700 transition-all">
                    Sync All Reports
                </button>
                <div id="syncStatus" class="text-sm text-slate-500 ml-4"></div>
            </div>
        </div>

        <!-- Column Visibility Controls -->
        <div class="bg-white rounded-2xl shadow-lg p-4 mb-6 border border-slate-200">
            <div class="flex flex-wrap items-center gap-2">
                <span class="text-sm font-bold text-slate-700 mr-2">Show Columns:</span>
                <label class="flex items-center gap-1 px-3 py-1 bg-slate-100 rounded-full cursor-pointer hover:bg-slate-200">
                    <input type="checkbox" class="col-toggle w-4 h-4" data-col="status" checked onchange="toggleColumn('status')">
                    <span class="text-xs font-medium text-slate-700">Status</span>
                </label>
                <label class="flex items-center gap-1 px-3 py-1 bg-slate-100 rounded-full cursor-pointer hover:bg-slate-200">
                    <input type="checkbox" class="col-toggle w-4 h-4" data-col="rt-inv" checked onchange="toggleColumn('rt-inv')">
                    <span class="text-xs font-medium text-slate-700">RT Inventory</span>
                </label>
                <label class="flex items-center gap-1 px-3 py-1 bg-slate-100 rounded-full cursor-pointer hover:bg-slate-200">
                    <input type="checkbox" class="col-toggle w-4 h-4" data-col="rt-sales" checked onchange="toggleColumn('rt-sales')">
                    <span class="text-xs font-medium text-slate-700">RT Sales</span>
                </label>
                <label class="flex items-center gap-1 px-3 py-1 bg-slate-100 rounded-full cursor-pointer hover:bg-slate-200">
                    <input type="checkbox" class="col-toggle w-4 h-4" data-col="sales" checked onchange="toggleColumn('sales')">
                    <span class="text-xs font-medium text-slate-700">Sales</span>
                </label>
                <label class="flex items-center gap-1 px-3 py-1 bg-slate-100 rounded-full cursor-pointer hover:bg-slate-200">
                    <input type="checkbox" class="col-toggle w-4 h-4" data-col="traffic" checked onchange="toggleColumn('traffic')">
                    <span class="text-xs font-medium text-slate-700">Traffic</span>
                </label>
                <label class="flex items-center gap-1 px-3 py-1 bg-slate-100 rounded-full cursor-pointer hover:bg-slate-200">
                    <input type="checkbox" class="col-toggle w-4 h-4" data-col="margin" checked onchange="toggleColumn('margin')">
                    <span class="text-xs font-medium text-slate-700">Margin</span>
                </label>
                <label class="flex items-center gap-1 px-3 py-1 bg-slate-100 rounded-full cursor-pointer hover:bg-slate-200">
                    <input type="checkbox" class="col-toggle w-4 h-4" data-col="inventory" checked onchange="toggleColumn('inventory')">
                    <span class="text-xs font-medium text-slate-700">Inventory</span>
                </label>
                <label class="flex items-center gap-1 px-3 py-1 bg-slate-100 rounded-full cursor-pointer hover:bg-slate-200">
                    <input type="checkbox" class="col-toggle w-4 h-4" data-col="last-ordered" checked onchange="toggleColumn('last-ordered')">
                    <span class="text-xs font-medium text-slate-700">Last Ordered</span>
                </label>
            </div>
        </div>

        <!-- Main Data Table -->
        <div class="bg-white rounded-2xl shadow-xl border border-slate-200 overflow-hidden">
            <div class="table-container max-h-[70vh] overflow-auto">
                <table class="w-full text-sm">
                    <thead class="bg-slate-900 text-white">
                        <tr>
                            <th class="px-2 py-3 text-center bg-slate-900" style="width: 40px;">
                                <input type="checkbox" id="selectAll" onchange="toggleSelectAll()" class="w-4 h-4 cursor-pointer">
                            </th>
                            <th class="sticky-col px-4 py-3 text-left font-bold bg-slate-900" style="left: 40px;">ASIN</th>
                            <th class="px-3 py-3 text-center font-bold bg-slate-900 col-status" style="min-width: 130px;">
                                <div>Product Status</div>
                                <div class="text-xs font-normal text-slate-400">Availability & Price</div>
                            </th>
                            <th class="report-cell px-3 py-3 text-center font-bold bg-slate-900 col-rt-inv" title="GET_VENDOR_REAL_TIME_INVENTORY_REPORT">
                                <div>RT Inventory</div>
                                <div class="text-xs font-normal text-slate-400">Highly Available</div>
                                <div class="text-[10px] font-normal text-blue-400 mt-1">Real-Time API</div>
                            </th>
                            <th class="report-cell px-3 py-3 text-center font-bold bg-slate-900 col-rt-sales" title="GET_VENDOR_REAL_TIME_SALES_REPORT">
                                <div>RT Sales</div>
                                <div class="text-xs font-normal text-slate-400">Units & Revenue</div>
                                <div class="text-[10px] font-normal text-blue-400 mt-1">Real-Time API</div>
                            </th>
                            <th class="report-cell px-3 py-3 text-center font-bold bg-slate-900 col-sales" title="GET_VENDOR_SALES_REPORT">
                                <div>Sales</div>
                                <div class="text-xs font-normal text-slate-400">Shipped & Ordered</div>
                                <div class="text-[10px] font-normal text-purple-400 mt-1">Weekly Report</div>
                            </th>
                            <th class="report-cell px-3 py-3 text-center font-bold bg-slate-900 col-traffic" title="GET_VENDOR_TRAFFIC_REPORT">
                                <div>Traffic</div>
                                <div class="text-xs font-normal text-slate-400">Glance Views</div>
                                <div class="text-[10px] font-normal text-purple-400 mt-1">Weekly Report</div>
                            </th>
                            <th class="report-cell px-3 py-3 text-center font-bold bg-slate-900 col-margin" title="GET_VENDOR_NET_PURE_PRODUCT_MARGIN_REPORT">
                                <div>Margin</div>
                                <div class="text-xs font-normal text-slate-400">Net PPM</div>
                                <div class="text-[10px] font-normal text-purple-400 mt-1">Weekly Report</div>
                            </th>
                            <th class="report-cell px-3 py-3 text-center font-bold bg-slate-900 col-inventory" title="GET_VENDOR_INVENTORY_REPORT">
                                <div>Inventory</div>
                                <div class="text-xs font-normal text-slate-400">On Hand</div>
                                <div class="text-[10px] font-normal text-purple-400 mt-1">Weekly Report</div>
                            </th>
                            <th class="px-3 py-3 text-center font-bold bg-slate-900 col-last-ordered" style="min-width: 120px;">
                                <div>Last Ordered</div>
                                <div class="text-xs font-normal text-slate-400">PO Date & Count</div>
                                <div class="text-[10px] font-normal text-emerald-400 mt-1">Purchase Orders</div>
                            </th>
                            <th class="px-4 py-3 text-center font-bold bg-slate-900">Actions</th>
                        </tr>
                    </thead>
                    <tbody class="divide-y divide-slate-200">
                        <% if (asins.length === 0) { %>
                        <tr>
                            <td colspan="11" class="px-4 py-12 text-center text-slate-500">
                                <p class="text-4xl mb-4">ðŸ“Š</p>
                                <p class="font-medium">No ASINs tracked yet</p>
                                <p class="text-sm">Add ASINs on the Products page to see analytics</p>
                            </td>
                        </tr>
                        <% } else { %>
                        <% asins.forEach(asin => {
                            const tracker = asinTrackerData[asin] || {};
                            const isUnavailable = tracker.availability === 'Unavailable';
                            const rtInv = dataByAsin[asin]?.['GET_VENDOR_REAL_TIME_INVENTORY_REPORT'];
                            const rtSales = dataByAsin[asin]?.['GET_VENDOR_REAL_TIME_SALES_REPORT'];
                            const sales = dataByAsin[asin]?.['GET_VENDOR_SALES_REPORT'];
                            const traffic = dataByAsin[asin]?.['GET_VENDOR_TRAFFIC_REPORT'];
                            const margin = dataByAsin[asin]?.['GET_VENDOR_NET_PURE_PRODUCT_MARGIN_REPORT'];
                            const inventory = dataByAsin[asin]?.['GET_VENDOR_INVENTORY_REPORT'];
                        %>
                        <tr class="hover:bg-slate-50 <%= isUnavailable ? 'unavailable-row' : '' %>" id="row-<%= asin %>">
                            <!-- Checkbox Column -->
                            <td class="px-2 py-3 text-center <%= isUnavailable ? 'bg-red-50' : 'bg-white' %>">
                                <input type="checkbox" class="asin-checkbox w-4 h-4 cursor-pointer" value="<%= asin %>" onchange="updateSelectAllState()">
                            </td>
                            <!-- ASIN Column -->
                            <td class="sticky-col px-4 py-3 <%= isUnavailable ? 'bg-red-50' : 'bg-white' %>" style="left: 40px;">
                                <a href="/catalog/<%= asin %>" class="font-mono font-bold text-blue-600 hover:text-blue-800 hover:underline">
                                    <%= asin %>
                                </a>
                                <% const title = productTitles[asin]; %>
                                <% const sku = vendorSkus[asin]; %>
                                <% if (title) { %>
                                <div class="text-xs text-slate-500 max-w-[180px] truncate" title="<%= title %>"><%= title %></div>
                                <% } %>
                                <% if (sku) { %>
                                <div class="text-[10px] text-slate-400">SKU: <%= sku %></div>
                                <% } %>
                            </td>

                            <!-- Product Status Column -->
                            <td class="px-3 py-3 text-center col-status">
                                <% if (tracker.availability) { %>
                                    <% if (tracker.availability === 'In Stock') { %>
                                        <span class="status-badge status-instock">In Stock</span>
                                    <% } else if (tracker.availability === 'Back Order') { %>
                                        <span class="status-badge status-backorder">Back Order</span>
                                    <% } else if (tracker.availability === 'Unavailable') { %>
                                        <span class="status-badge status-unavailable">Unavailable</span>
                                    <% } else { %>
                                        <span class="status-badge status-nodata"><%= tracker.availability %></span>
                                    <% } %>
                                    <% if (tracker.price && tracker.price !== 'N/A') { %>
                                        <div class="price-display"><%= tracker.price %></div>
                                    <% } %>
                                <% } else { %>
                                    <span class="status-badge status-nodata">No Data</span>
                                <% } %>
                            </td>

                            <!-- RT Inventory Column -->
                            <td class="report-cell px-3 py-3 text-center col-rt-inv">
                                <% if (rtInv) {
                                    const data = typeof rtInv === 'string' ? JSON.parse(rtInv) : rtInv;
                                %>
                                    <% if (data.highlyAvailableInventory !== undefined) { %>
                                        <span class="status-badge <%= data.highlyAvailableInventory ? 'status-instock' : 'status-unavailable' %>">
                                            <%= data.highlyAvailableInventory ? 'Yes' : 'No' %>
                                        </span>
                                    <% } else { %>
                                        <span class="text-slate-400">-</span>
                                    <% } %>
                                <% } else { %>
                                    <span class="text-slate-400 text-xs">No data</span>
                                <% } %>
                            </td>

                            <!-- RT Sales Column -->
                            <td class="report-cell px-3 py-3 col-rt-sales">
                                <% if (rtSales) {
                                    const data = typeof rtSales === 'string' ? JSON.parse(rtSales) : rtSales;
                                %>
                                    <div class="metric-group">
                                        <div class="metric-item">
                                            <div class="metric-label">Units</div>
                                            <div class="metric-value text-blue-600"><%= data.orderedUnits !== undefined ? data.orderedUnits : '-' %></div>
                                        </div>
                                        <div class="metric-item">
                                            <div class="metric-label">Revenue</div>
                                            <div class="metric-value text-green-600">
                                                <% if (data.orderedRevenue?.amount !== undefined) { %>$<%= parseFloat(data.orderedRevenue.amount).toFixed(0) %><% } else { %>-<% } %>
                                            </div>
                                        </div>
                                    </div>
                                <% } else { %>
                                    <span class="text-slate-400">-</span>
                                <% } %>
                            </td>

                            <!-- Sales Column -->
                            <td class="report-cell px-3 py-3 col-sales">
                                <% if (sales) {
                                    const data = typeof sales === 'string' ? JSON.parse(sales) : sales;
                                %>
                                    <div class="metric-group">
                                        <div class="metric-item">
                                            <div class="metric-label">Shipped</div>
                                            <div class="metric-value text-purple-600"><%= data.shippedUnits !== undefined ? data.shippedUnits : '-' %></div>
                                        </div>
                                        <div class="metric-item">
                                            <div class="metric-label">Ordered</div>
                                            <div class="metric-value text-blue-600"><%= data.orderedUnits !== undefined ? data.orderedUnits : '-' %></div>
                                        </div>
                                        <% if (data.orderedRevenue?.amount !== undefined) { %>
                                        <div class="metric-item">
                                            <div class="metric-label">Revenue</div>
                                            <div class="metric-value text-green-600">$<%= parseFloat(data.orderedRevenue.amount).toFixed(0) %></div>
                                        </div>
                                        <% } %>
                                    </div>
                                <% } else { %>
                                    <span class="text-slate-400">-</span>
                                <% } %>
                            </td>

                            <!-- Traffic Column -->
                            <td class="report-cell px-3 py-3 text-center col-traffic">
                                <% if (traffic) {
                                    const data = typeof traffic === 'string' ? JSON.parse(traffic) : traffic;
                                %>
                                    <div class="metric-item">
                                        <div class="metric-label">Views</div>
                                        <div class="metric-value text-orange-600"><%= data.glanceViews !== undefined ? data.glanceViews.toLocaleString() : '-' %></div>
                                    </div>
                                <% } else { %>
                                    <span class="text-slate-400">-</span>
                                <% } %>
                            </td>

                            <!-- Margin Column -->
                            <td class="report-cell px-3 py-3 text-center col-margin">
                                <% if (margin) {
                                    const data = typeof margin === 'string' ? JSON.parse(margin) : margin;
                                %>
                                    <% if (data.netPureProductMargin !== undefined) { %>
                                        <div class="metric-item">
                                            <div class="metric-label">Net PPM</div>
                                            <div class="metric-value <%= data.netPureProductMargin >= 0 ? 'text-teal-600' : 'text-red-600' %>">
                                                <%= (data.netPureProductMargin * 100).toFixed(1) %>%
                                            </div>
                                        </div>
                                    <% } else { %>
                                        <span class="text-slate-400">-</span>
                                    <% } %>
                                <% } else { %>
                                    <span class="text-slate-400">-</span>
                                <% } %>
                            </td>

                            <!-- Inventory Column -->
                            <td class="report-cell px-3 py-3 col-inventory">
                                <% if (inventory) {
                                    const data = typeof inventory === 'string' ? JSON.parse(inventory) : inventory;
                                %>
                                    <div class="metric-group">
                                        <div class="metric-item">
                                            <div class="metric-label">Sellable</div>
                                            <div class="metric-value text-indigo-600"><%= data.sellableOnHandInventoryUnits !== undefined ? data.sellableOnHandInventoryUnits : '-' %></div>
                                        </div>
                                        <% if (data.unsellableOnHandInventoryUnits !== undefined && data.unsellableOnHandInventoryUnits > 0) { %>
                                        <div class="metric-item">
                                            <div class="metric-label">Unsellable</div>
                                            <div class="metric-value text-red-500"><%= data.unsellableOnHandInventoryUnits %></div>
                                        </div>
                                        <% } %>
                                    </div>
                                <% } else { %>
                                    <span class="text-slate-400">-</span>
                                <% } %>
                            </td>

                            <!-- Last Ordered Column -->
                            <td class="px-3 py-3 text-center col-last-ordered">
                                <% const lastOrderedData = lastOrderedByAsin[asin]; %>
                                <% if (lastOrderedData && lastOrderedData.date) { %>
                                    <div class="metric-item">
                                        <div class="metric-label">Last PO</div>
                                        <div class="metric-value text-slate-700 text-sm"><%= new Date(lastOrderedData.date).toLocaleDateString('en-US', { month: 'short', day: 'numeric', year: 'numeric' }) %></div>
                                    </div>
                                    <div class="text-xs text-slate-500 mt-1">
                                        <button onclick="showAsinPOs('<%= asin %>')" class="text-blue-600 hover:underline">
                                            <%= lastOrderedData.poCount %> PO<%= lastOrderedData.poCount !== 1 ? 's' : '' %>
                                        </button>
                                    </div>
                                <% } else { %>
                                    <span class="text-slate-400">-</span>
                                <% } %>
                            </td>

                            <!-- Actions Column -->
                            <td class="px-4 py-3 text-center">
                                <div class="flex gap-1 justify-center">
                                    <button onclick="showAsinDetail('<%= asin %>')" class="bg-blue-100 text-blue-700 px-2 py-1 rounded text-xs font-medium hover:bg-blue-200" title="View detailed report">
                                        Details
                                    </button>
                                    <a href="/catalog/<%= asin %>" class="bg-slate-100 text-slate-700 px-2 py-1 rounded text-xs font-medium hover:bg-slate-200" title="View catalog info">
                                        Catalog
                                    </a>
                                </div>
                            </td>
                        </tr>
                        <% }); %>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>

        <!-- Performance Charts Section -->
        <div class="bg-white rounded-2xl shadow-xl border border-slate-200 p-6 mt-8">
            <div class="flex flex-wrap justify-between items-center gap-4 mb-6">
                <div>
                    <h3 class="text-xl font-bold text-slate-900">Performance Charts</h3>
                    <p class="text-sm text-slate-500">Visualize metrics over time for selected ASINs</p>
                </div>
                <div class="flex flex-wrap items-center gap-3">
                    <select id="chartMetric" onchange="updateChart()" class="px-3 py-2 rounded-lg border border-slate-300 text-sm">
                        <option value="orderedUnits">Ordered Units (Weekly)</option>
                        <option value="shippedUnits">Shipped Units (Weekly)</option>
                        <option value="orderedRevenue">Revenue (Weekly)</option>
                        <option value="glanceViews">Traffic / Views (Weekly)</option>
                        <option value="sellableOnHandInventoryUnits">Inventory Units (Weekly)</option>
                        <option value="netPureProductMargin">Margin % (Weekly)</option>
                        <option value="rtOrderedUnits">RT Ordered Units</option>
                        <option value="rtOrderedRevenue">RT Revenue</option>
                    </select>
                    <label class="flex items-center gap-2 px-3 py-2 bg-slate-100 rounded-lg cursor-pointer">
                        <input type="checkbox" id="mergeAsins" onchange="updateChart()" class="w-4 h-4">
                        <span class="text-sm font-medium text-slate-700">Merge ASINs</span>
                    </label>
                    <button onclick="updateChart()" class="bg-blue-600 text-white px-4 py-2 rounded-lg text-sm font-bold hover:bg-blue-700">
                        Refresh Chart
                    </button>
                </div>
            </div>
            <div class="mb-4">
                <div class="text-sm font-medium text-slate-700 mb-2">Select ASINs to chart (check boxes in the table above, or select here):</div>
                <div id="chartAsinSelector" class="flex flex-wrap gap-2 max-h-24 overflow-auto p-2 bg-slate-50 rounded-lg">
                    <% asins.forEach(asin => { %>
                    <label class="flex items-center gap-1 px-2 py-1 bg-white border border-slate-200 rounded cursor-pointer hover:bg-blue-50">
                        <input type="checkbox" class="chart-asin-checkbox w-3 h-3" value="<%= asin %>">
                        <span class="text-xs font-mono"><%= asin %></span>
                    </label>
                    <% }); %>
                </div>
            </div>
            <div id="chartContainer" class="relative" style="height: 350px;">
                <canvas id="analyticsChart"></canvas>
                <div id="chartPlaceholder" class="absolute inset-0 flex items-center justify-center bg-slate-50 rounded-lg">
                    <div class="text-center text-slate-500">
                        <p class="text-lg font-medium">Select ASINs to display chart</p>
                        <p class="text-sm">Use checkboxes in the table or the selector above</p>
                    </div>
                </div>
            </div>
        </div>

        <!-- Data Source Info Panel -->
        <div class="bg-white rounded-2xl shadow-lg border border-slate-200 p-6 mt-6">
            <h3 class="text-lg font-bold text-slate-900 mb-4">Data Source Information</h3>
            <div class="grid grid-cols-1 md:grid-cols-3 gap-4">
                <% Object.keys(reportTypes).forEach(rt => {
                    const meta = reportMetadata[rt];
                    const config = reportTypes[rt];
                %>
                <div class="border border-slate-200 rounded-lg p-3">
                    <div class="font-medium text-slate-800"><%= config.name %></div>
                    <div class="text-xs text-slate-500 mt-1">
                        <% if (meta) { %>
                            Data: <%= new Date(meta.minDate).toLocaleDateString('en-US', { month: 'short', day: 'numeric' }) %> - <%= new Date(meta.maxDate).toLocaleDateString('en-US', { month: 'short', day: 'numeric', year: 'numeric' }) %>
                            <% if (meta.lastSync) { %>
                            <br>Last synced: <%= new Date(meta.lastSync).toLocaleString() %>
                            <% } %>
                        <% } else { %>
                            No data synced for this period
                        <% } %>
                    </div>
                    <div class="text-[10px] text-slate-400 mt-1 font-mono"><%= rt %></div>
                </div>
                <% }); %>
            </div>
        </div>

        <!-- ASIN POs Modal -->
        <div id="asinPOsModal" class="fixed inset-0 bg-black bg-opacity-50 hidden items-center justify-center z-50">
            <div class="bg-white rounded-2xl p-6 max-w-3xl w-full mx-4 shadow-2xl max-h-[90vh] overflow-auto">
                <div class="flex justify-between items-center mb-4">
                    <h3 class="text-xl font-bold text-slate-900">
                        Purchase Orders for <span id="poAsin" class="font-mono text-blue-600"></span>
                    </h3>
                    <button onclick="closeAsinPOsModal()" class="text-slate-400 hover:text-slate-600 text-2xl">&times;</button>
                </div>
                <div id="asinPOsContent">
                    <div class="text-center py-8 text-slate-500">Loading...</div>
                </div>
                <div class="mt-6 flex justify-end">
                    <button onclick="closeAsinPOsModal()" class="bg-slate-600 text-white px-4 py-2 rounded-lg font-medium hover:bg-slate-700">
                        Close
                    </button>
                </div>
            </div>
        </div>

        <!-- ASIN Detail Modal -->
        <div id="asinDetailModal" class="fixed inset-0 bg-black bg-opacity-50 hidden items-center justify-center z-50">
            <div class="bg-white rounded-2xl p-6 max-w-2xl w-full mx-4 shadow-2xl max-h-[90vh] overflow-auto">
                <div class="flex justify-between items-center mb-4">
                    <h3 class="text-xl font-bold text-slate-900">
                        ASIN: <span id="detailAsin" class="font-mono text-blue-600"></span>
                    </h3>
                    <button onclick="closeAsinDetail()" class="text-slate-400 hover:text-slate-600 text-2xl">&times;</button>
                </div>

                <!-- Product Status -->
                <div id="detailStatus" class="mb-6 p-4 bg-slate-50 rounded-xl"></div>

                <!-- Report Sections -->
                <div class="space-y-4">
                    <div class="border border-slate-200 rounded-xl overflow-hidden">
                        <div class="bg-slate-100 px-4 py-2 font-bold text-slate-700">Real-Time Inventory</div>
                        <div id="detailRtInv" class="p-4"></div>
                    </div>
                    <div class="border border-slate-200 rounded-xl overflow-hidden">
                        <div class="bg-slate-100 px-4 py-2 font-bold text-slate-700">Real-Time Sales</div>
                        <div id="detailRtSales" class="p-4"></div>
                    </div>
                    <div class="border border-slate-200 rounded-xl overflow-hidden">
                        <div class="bg-slate-100 px-4 py-2 font-bold text-slate-700">Sales Report</div>
                        <div id="detailSales" class="p-4"></div>
                    </div>
                    <div class="border border-slate-200 rounded-xl overflow-hidden">
                        <div class="bg-slate-100 px-4 py-2 font-bold text-slate-700">Traffic Report</div>
                        <div id="detailTraffic" class="p-4"></div>
                    </div>
                    <div class="border border-slate-200 rounded-xl overflow-hidden">
                        <div class="bg-slate-100 px-4 py-2 font-bold text-slate-700">Net Pure Product Margin</div>
                        <div id="detailMargin" class="p-4"></div>
                    </div>
                    <div class="border border-slate-200 rounded-xl overflow-hidden">
                        <div class="bg-slate-100 px-4 py-2 font-bold text-slate-700">Inventory Report</div>
                        <div id="detailInventory" class="p-4"></div>
                    </div>
                </div>

                <div class="mt-6 flex justify-end gap-3">
                    <a id="detailCatalogLink" href="#" class="bg-slate-200 text-slate-700 px-4 py-2 rounded-lg font-medium hover:bg-slate-300 transition-all">
                        View Catalog
                    </a>
                    <button onclick="closeAsinDetail()" class="bg-blue-600 text-white px-4 py-2 rounded-lg font-medium hover:bg-blue-700 transition-all">
                        Close
                    </button>
                </div>
            </div>
        </div>

        <!-- Sync Progress Modal -->
        <div id="syncModal" class="fixed inset-0 bg-black bg-opacity-50 hidden items-center justify-center z-50">
            <div class="bg-white rounded-2xl p-8 max-w-lg w-full mx-4 shadow-2xl">
                <h3 class="text-xl font-bold text-slate-900 mb-4">Syncing Reports from SP-API</h3>
                <div id="syncProgress" class="space-y-3">
                    <!-- Progress items will be inserted here -->
                </div>
                <div class="mt-6 flex justify-end">
                    <button onclick="closeSyncModal()" id="closeSyncBtn" class="bg-slate-600 text-white px-6 py-2 rounded-lg font-bold hover:bg-slate-700 transition-all hidden">
                        Close
                    </button>
                </div>
            </div>
        </div>
    </div>

    <script>
        const reportTypes = <%- JSON.stringify(reportTypes) %>;
        const reportTypeKeys = Object.keys(reportTypes);
        const dataByAsin = <%- JSON.stringify(dataByAsin) %>;
        const asinTrackerData = <%- JSON.stringify(asinTrackerData) %>;
        const lastOrderedByAsin = <%- JSON.stringify(lastOrderedByAsin) %>;
        const productTitles = <%- JSON.stringify(productTitles || {}) %>;
        const vendorSkus = <%- JSON.stringify(vendorSkus || {}) %>;
        const reportMetadata = <%- JSON.stringify(reportMetadata || {}) %>;
        const startDate = '<%= startDate %>';
        const endDate = '<%= endDate %>';

        function applyDateFilter() {
            const startDate = document.getElementById('startDate').value;
            const endDate = document.getElementById('endDate').value;
            window.location.href = `/vendor-analytics?startDate=${startDate}&endDate=${endDate}`;
        }

        // Checkbox functions
        function toggleSelectAll() {
            const selectAll = document.getElementById('selectAll');
            const checkboxes = document.querySelectorAll('.asin-checkbox');
            checkboxes.forEach(cb => {
                cb.checked = selectAll.checked;
            });
            updateSyncSelectedButton();
        }

        function updateSelectAllState() {
            const checkboxes = document.querySelectorAll('.asin-checkbox');
            const selectAll = document.getElementById('selectAll');
            const checkedCount = document.querySelectorAll('.asin-checkbox:checked').length;
            selectAll.checked = checkedCount === checkboxes.length && checkboxes.length > 0;
            selectAll.indeterminate = checkedCount > 0 && checkedCount < checkboxes.length;
            updateSyncSelectedButton();
        }

        function updateSyncSelectedButton() {
            const checkedCount = document.querySelectorAll('.asin-checkbox:checked').length;
            const btn = document.getElementById('syncSelectedBtn');
            btn.disabled = checkedCount === 0;
            btn.textContent = checkedCount > 0 ? `Sync Selected (${checkedCount})` : 'Sync Selected';
        }

        async function syncSelectedAsins() {
            const checkedBoxes = document.querySelectorAll('.asin-checkbox:checked');
            if (checkedBoxes.length === 0) {
                alert('Please select at least one ASIN');
                return;
            }

            const asins = Array.from(checkedBoxes).map(cb => cb.value);
            if (!confirm(`Sync reports for ${asins.length} selected ASIN${asins.length > 1 ? 's' : ''}?\n\nNote: Amazon reports include all ASINs, but this will refresh all data.`)) {
                return;
            }

            // Use the same sync modal as refreshAllReports
            refreshAllReports();
        }

        // Column visibility toggle
        function toggleColumn(colName) {
            const cells = document.querySelectorAll(`.col-${colName}`);
            const checkbox = document.querySelector(`.col-toggle[data-col="${colName}"]`);
            const isVisible = checkbox.checked;

            cells.forEach(cell => {
                cell.style.display = isVisible ? '' : 'none';
            });

            // Save preference to localStorage
            const prefs = JSON.parse(localStorage.getItem('vendorAnalyticsColumns') || '{}');
            prefs[colName] = isVisible;
            localStorage.setItem('vendorAnalyticsColumns', JSON.stringify(prefs));
        }

        // Restore column visibility from localStorage on page load
        function restoreColumnVisibility() {
            const prefs = JSON.parse(localStorage.getItem('vendorAnalyticsColumns') || '{}');
            Object.keys(prefs).forEach(colName => {
                const checkbox = document.querySelector(`.col-toggle[data-col="${colName}"]`);
                if (checkbox) {
                    checkbox.checked = prefs[colName];
                    toggleColumn(colName);
                }
            });
        }

        // Initialize column visibility on page load
        restoreColumnVisibility();

        function closeSyncModal() {
            document.getElementById('syncModal').classList.add('hidden');
            document.getElementById('syncModal').classList.remove('flex');
            // Refresh page to show new data
            window.location.reload();
        }

        async function refreshAllReports() {
            const startDate = document.getElementById('startDate').value;
            const endDate = document.getElementById('endDate').value;

            // Show modal
            document.getElementById('syncModal').classList.remove('hidden');
            document.getElementById('syncModal').classList.add('flex');
            document.getElementById('closeSyncBtn').classList.add('hidden');

            const progressDiv = document.getElementById('syncProgress');
            progressDiv.innerHTML = '';

            let hasErrors = false;

            for (const reportType of reportTypeKeys) {
                const itemId = `sync-${reportType}`;
                progressDiv.innerHTML += `
                    <div id="${itemId}" class="flex items-center gap-3 p-3 bg-slate-100 rounded-lg">
                        <div class="loading-spinner"></div>
                        <div>
                            <div class="font-medium text-slate-800">${reportTypes[reportType].name}</div>
                            <div class="text-xs text-slate-500">Creating report request...</div>
                        </div>
                    </div>
                `;

                try {
                    // Step 1: Create report request
                    const createResp = await fetch('/api/vendor-reports/create', {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({
                            reportType,
                            startDate,
                            endDate
                        })
                    });

                    const createData = await createResp.json();

                    if (!createData.success) {
                        throw new Error(createData.error || 'Failed to create report');
                    }

                    const reportId = createData.reportId;
                    updateSyncItem(itemId, 'loading', `Report ID: ${reportId} - Checking status...`);

                    // Step 2: Poll for status (max 30 attempts, 2 second intervals)
                    let reportDocumentId = null;
                    for (let i = 0; i < 30; i++) {
                        await sleep(2000);

                        const statusResp = await fetch(`/api/vendor-reports/status/${reportId}`);
                        const statusData = await statusResp.json();

                        if (statusData.status === 'DONE') {
                            reportDocumentId = statusData.reportDocumentId;
                            updateSyncItem(itemId, 'loading', 'Report ready - Downloading...');
                            break;
                        } else if (statusData.status === 'CANCELLED' || statusData.status === 'FATAL') {
                            throw new Error(`Report ${statusData.status}`);
                        }

                        updateSyncItem(itemId, 'loading', `Status: ${statusData.status} (attempt ${i + 1}/30)`);
                    }

                    if (!reportDocumentId) {
                        throw new Error('Report timed out');
                    }

                    // Step 3: Download and save report
                    const downloadResp = await fetch(
                        `/api/vendor-reports/download/${reportDocumentId}?reportType=${reportType}&saveToDb=true`
                    );
                    const downloadData = await downloadResp.json();

                    if (!downloadData.success) {
                        throw new Error('Failed to download report');
                    }

                    updateSyncItem(itemId, 'success', 'Synced successfully');

                } catch (err) {
                    hasErrors = true;
                    updateSyncItem(itemId, 'error', err.message);
                }
            }

            // Show close button
            document.getElementById('closeSyncBtn').classList.remove('hidden');
        }

        function updateSyncItem(itemId, status, message) {
            const item = document.getElementById(itemId);
            if (!item) return;

            const statusDiv = item.querySelector('div:first-child');
            const messageDiv = item.querySelector('.text-xs');

            if (status === 'success') {
                statusDiv.innerHTML = '<span class="text-2xl">OK</span>';
                statusDiv.className = 'text-green-600 font-bold';
                item.className = 'flex items-center gap-3 p-3 bg-green-50 rounded-lg border border-green-200';
            } else if (status === 'error') {
                statusDiv.innerHTML = '<span class="text-2xl">X</span>';
                statusDiv.className = 'text-red-600 font-bold';
                item.className = 'flex items-center gap-3 p-3 bg-red-50 rounded-lg border border-red-200';
            }

            if (messageDiv) {
                messageDiv.textContent = message;
            }
        }

        function sleep(ms) {
            return new Promise(resolve => setTimeout(resolve, ms));
        }

        // ASIN Detail Modal Functions
        function showAsinDetail(asin) {
            document.getElementById('detailAsin').textContent = asin;
            document.getElementById('detailCatalogLink').href = `/catalog/${asin}`;

            // Product Status
            const tracker = asinTrackerData[asin] || {};
            let statusHtml = '';
            if (tracker.availability) {
                let badgeClass = 'status-nodata';
                if (tracker.availability === 'In Stock') badgeClass = 'status-instock';
                else if (tracker.availability === 'Back Order') badgeClass = 'status-backorder';
                else if (tracker.availability === 'Unavailable') badgeClass = 'status-unavailable';

                const lastOrdered = lastOrderedByAsin[asin];
                const lastOrderedStr = lastOrdered ? new Date(lastOrdered).toLocaleDateString('en-US', { month: 'short', day: 'numeric', year: 'numeric' }) : null;

                statusHtml = `<div class="flex flex-wrap items-center gap-4">
                    <div><span class="status-badge ${badgeClass}">${tracker.availability}</span></div>
                    ${tracker.price && tracker.price !== 'N/A' ? `<div class="text-xl font-bold text-slate-700">${tracker.price}</div>` : ''}
                    ${lastOrderedStr ? `<div class="text-sm"><span class="text-slate-500">Last Ordered:</span> <span class="font-medium">${lastOrderedStr}</span></div>` : ''}
                </div>
                ${tracker.header ? `<div class="text-sm text-slate-500 mt-2">${tracker.header}</div>` : ''}`;
            } else {
                statusHtml = '<span class="text-slate-400">No tracker data available</span>';
            }
            document.getElementById('detailStatus').innerHTML = statusHtml;

            // RT Inventory
            const rtInv = dataByAsin[asin]?.['GET_VENDOR_REAL_TIME_INVENTORY_REPORT'];
            document.getElementById('detailRtInv').innerHTML = rtInv
                ? formatMetric('Highly Available Inventory', rtInv.highlyAvailableInventory !== undefined
                    ? (rtInv.highlyAvailableInventory ? '<span class="text-green-600 font-bold">Yes</span>' : '<span class="text-red-600 font-bold">No</span>')
                    : '-')
                : noDataHtml();

            // RT Sales
            const rtSales = dataByAsin[asin]?.['GET_VENDOR_REAL_TIME_SALES_REPORT'];
            document.getElementById('detailRtSales').innerHTML = rtSales
                ? `<div class="grid grid-cols-2 gap-4">
                    ${formatMetric('Ordered Units', rtSales.orderedUnits ?? '-')}
                    ${formatMetric('Ordered Revenue', rtSales.orderedRevenue?.amount ? '$' + parseFloat(rtSales.orderedRevenue.amount).toFixed(2) : '-')}
                   </div>`
                : noDataHtml();

            // Sales
            const sales = dataByAsin[asin]?.['GET_VENDOR_SALES_REPORT'];
            document.getElementById('detailSales').innerHTML = sales
                ? `<div class="grid grid-cols-2 md:grid-cols-3 gap-4">
                    ${formatMetric('Shipped Units', sales.shippedUnits ?? '-')}
                    ${formatMetric('Shipped Revenue', sales.shippedRevenue?.amount ? '$' + parseFloat(sales.shippedRevenue.amount).toFixed(2) : '-')}
                    ${formatMetric('Shipped COGS', sales.shippedCogs?.amount ? '$' + parseFloat(sales.shippedCogs.amount).toFixed(2) : '-')}
                    ${formatMetric('Ordered Units', sales.orderedUnits ?? '-')}
                    ${formatMetric('Ordered Revenue', sales.orderedRevenue?.amount ? '$' + parseFloat(sales.orderedRevenue.amount).toFixed(2) : '-')}
                   </div>`
                : noDataHtml();

            // Traffic
            const traffic = dataByAsin[asin]?.['GET_VENDOR_TRAFFIC_REPORT'];
            document.getElementById('detailTraffic').innerHTML = traffic
                ? formatMetric('Glance Views', traffic.glanceViews?.toLocaleString() ?? '-')
                : noDataHtml();

            // Margin
            const margin = dataByAsin[asin]?.['GET_VENDOR_NET_PURE_PRODUCT_MARGIN_REPORT'];
            document.getElementById('detailMargin').innerHTML = margin
                ? formatMetric('Net Pure Product Margin', margin.netPureProductMargin !== undefined
                    ? `<span class="${margin.netPureProductMargin >= 0 ? 'text-green-600' : 'text-red-600'} font-bold">${(margin.netPureProductMargin * 100).toFixed(2)}%</span>`
                    : '-')
                : noDataHtml();

            // Inventory
            const inventory = dataByAsin[asin]?.['GET_VENDOR_INVENTORY_REPORT'];
            document.getElementById('detailInventory').innerHTML = inventory
                ? `<div class="grid grid-cols-2 md:grid-cols-3 gap-4">
                    ${formatMetric('Sellable On Hand Units', inventory.sellableOnHandInventoryUnits ?? '-')}
                    ${formatMetric('Sellable On Hand Cost', inventory.sellableOnHandInventoryCost?.amount ? '$' + parseFloat(inventory.sellableOnHandInventoryCost.amount).toFixed(2) : '-')}
                    ${formatMetric('Unsellable On Hand Units', inventory.unsellableOnHandInventoryUnits ?? '-')}
                   </div>`
                : noDataHtml();

            // Show modal
            document.getElementById('asinDetailModal').classList.remove('hidden');
            document.getElementById('asinDetailModal').classList.add('flex');
        }

        function closeAsinDetail() {
            document.getElementById('asinDetailModal').classList.add('hidden');
            document.getElementById('asinDetailModal').classList.remove('flex');
        }

        function formatMetric(label, value) {
            return `<div class="metric-item">
                <div class="metric-label">${label}</div>
                <div class="metric-value text-lg">${value}</div>
            </div>`;
        }

        function noDataHtml() {
            return '<span class="text-slate-400">No data available for this period</span>';
        }

        // Chart functionality
        let chart = null;
        const chartColors = ['#3b82f6', '#10b981', '#f59e0b', '#ef4444', '#8b5cf6', '#ec4899', '#06b6d4', '#84cc16'];

        async function updateChart() {
            const selectedAsins = getSelectedChartAsins();
            if (selectedAsins.length === 0) {
                document.getElementById('chartPlaceholder').classList.remove('hidden');
                return;
            }

            document.getElementById('chartPlaceholder').classList.add('hidden');

            const metric = document.getElementById('chartMetric').value;
            const mergeAsins = document.getElementById('mergeAsins').checked;

            try {
                const response = await fetch(`/api/vendor-analytics/chart-data?asins=${selectedAsins.join(',')}&metric=${metric}&startDate=${startDate}&endDate=${endDate}&merge=${mergeAsins}`);
                const data = await response.json();

                if (!data.success) {
                    console.error('Chart data error:', data.error);
                    return;
                }

                if (chart) chart.destroy();

                const ctx = document.getElementById('analyticsChart').getContext('2d');
                chart = new Chart(ctx, {
                    type: 'line',
                    data: {
                        labels: data.dates,
                        datasets: data.series.map((s, i) => ({
                            label: s.label,
                            data: s.values,
                            borderColor: chartColors[i % chartColors.length],
                            backgroundColor: chartColors[i % chartColors.length] + '20',
                            tension: 0.3,
                            fill: true
                        }))
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        plugins: {
                            legend: { position: 'bottom' },
                            tooltip: {
                                callbacks: {
                                    label: (ctx) => {
                                        let value = ctx.raw;
                                        if (metric.includes('Revenue')) value = '$' + value.toFixed(2);
                                        else if (metric.includes('Margin')) value = (value * 100).toFixed(1) + '%';
                                        else value = value.toLocaleString();
                                        return ctx.dataset.label + ': ' + value;
                                    }
                                }
                            }
                        },
                        scales: {
                            x: {
                                type: 'category',
                                title: { display: true, text: 'Date' }
                            },
                            y: {
                                beginAtZero: !metric.includes('Margin'),
                                title: { display: true, text: metric }
                            }
                        }
                    }
                });
            } catch (err) {
                console.error('Chart update error:', err);
            }
        }

        function getSelectedChartAsins() {
            // First check the main table checkboxes
            const tableChecked = document.querySelectorAll('.asin-checkbox:checked');
            if (tableChecked.length > 0) {
                return Array.from(tableChecked).map(cb => cb.value);
            }
            // Fall back to chart ASIN selector
            const chartChecked = document.querySelectorAll('.chart-asin-checkbox:checked');
            return Array.from(chartChecked).map(cb => cb.value);
        }

        // Sync table checkboxes with chart ASIN selector
        document.querySelectorAll('.asin-checkbox').forEach(cb => {
            cb.addEventListener('change', () => {
                const chartCb = document.querySelector(`.chart-asin-checkbox[value="${cb.value}"]`);
                if (chartCb) chartCb.checked = cb.checked;
            });
        });

        // ASIN POs Modal
        async function showAsinPOs(asin) {
            document.getElementById('poAsin').textContent = asin;
            document.getElementById('asinPOsContent').innerHTML = '<div class="text-center py-8 text-slate-500">Loading...</div>';
            document.getElementById('asinPOsModal').classList.remove('hidden');
            document.getElementById('asinPOsModal').classList.add('flex');

            try {
                const response = await fetch(`/api/purchase-orders/by-asin/${asin}`);
                const data = await response.json();

                if (!data.success || !data.purchaseOrders.length) {
                    document.getElementById('asinPOsContent').innerHTML = '<div class="text-center py-8 text-slate-500">No purchase orders found for this ASIN</div>';
                    return;
                }

                const rows = data.purchaseOrders.map(po => `
                    <tr class="hover:bg-slate-50">
                        <td class="px-3 py-2 font-mono font-bold text-blue-600">
                            <a href="/purchase-orders" class="hover:underline">${po.po_number}</a>
                        </td>
                        <td class="px-3 py-2">${new Date(po.po_date).toLocaleDateString('en-US', { month: 'short', day: 'numeric', year: 'numeric' })}</td>
                        <td class="px-3 py-2">
                            <span class="px-2 py-1 rounded text-xs font-medium ${
                                po.po_status === 'Shipped' || po.po_status === 'Closed' ? 'bg-green-100 text-green-700' :
                                po.po_status === 'Acknowledged' ? 'bg-amber-100 text-amber-700' :
                                po.po_status === 'Cancelled' ? 'bg-red-100 text-red-700' :
                                'bg-blue-100 text-blue-700'
                            }">${po.po_status}</span>
                        </td>
                        <td class="px-3 py-2 text-right">${po.ordered_quantity || '-'}</td>
                        <td class="px-3 py-2 text-right">${po.acknowledged_quantity || '-'}</td>
                        <td class="px-3 py-2 text-right">${po.net_cost_amount ? '$' + parseFloat(po.net_cost_amount).toFixed(2) : '-'}</td>
                        <td class="px-3 py-2 text-xs text-slate-500">${po.vendor_sku || '-'}</td>
                    </tr>
                `).join('');

                document.getElementById('asinPOsContent').innerHTML = `
                    <div class="overflow-x-auto">
                        <table class="w-full text-sm border border-slate-200 rounded-lg">
                            <thead class="bg-slate-100">
                                <tr>
                                    <th class="px-3 py-2 text-left">PO Number</th>
                                    <th class="px-3 py-2 text-left">PO Date</th>
                                    <th class="px-3 py-2 text-left">Status</th>
                                    <th class="px-3 py-2 text-right">Ordered</th>
                                    <th class="px-3 py-2 text-right">Accepted</th>
                                    <th class="px-3 py-2 text-right">Unit Cost</th>
                                    <th class="px-3 py-2 text-left">Vendor SKU</th>
                                </tr>
                            </thead>
                            <tbody class="divide-y divide-slate-200">
                                ${rows}
                            </tbody>
                        </table>
                    </div>
                    <div class="mt-4 text-sm text-slate-500">
                        Total: ${data.purchaseOrders.length} purchase order${data.purchaseOrders.length !== 1 ? 's' : ''}
                    </div>
                `;
            } catch (err) {
                document.getElementById('asinPOsContent').innerHTML = '<div class="text-center py-8 text-red-500">Error loading purchase orders</div>';
            }
        }

        function closeAsinPOsModal() {
            document.getElementById('asinPOsModal').classList.add('hidden');
            document.getElementById('asinPOsModal').classList.remove('flex');
        }

    </script>
</body>
</html>
