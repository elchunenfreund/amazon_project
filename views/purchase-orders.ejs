                            <div><span class="text-slate-500">International Commercial Terms:</span> <span class="font-medium">${rawData.importDetails.internationalCommercialTerms || '-'}</span></div>
                            <div><span class="text-slate-500">Port of Delivery:</span> <span class="font-medium">${rawData.importDetails.portOfDelivery || '-'}</span></div>
                            <div><span class="text-slate-500">Import Containers:</span> <span class="font-medium">${rawData.importDetails.importContainers || '-'}</span></div>
                            <div><span class="text-slate-500">Shipping Instructions:</span> <span class="font-medium">${rawData.importDetails.shippingInstructions || '-'}</span></div>
                        </div>
                    </div>
                </details>
                ` : ''}

                <!-- Payment Terms -->
                ${rawData.paymentTerms ? `
                <details class="mt-4 border border-slate-200 rounded-lg">
                    <summary class="cursor-pointer p-3 bg-slate-50 font-bold text-slate-700 hover:bg-slate-100 rounded-t-lg">
                        Payment Terms
                    </summary>
                    <div class="p-4 text-sm">
                        <div class="grid grid-cols-2 gap-2">
                            <div><span class="text-slate-500">Type:</span> <span class="font-medium">${rawData.paymentTerms.type || '-'}</span></div>
                            <div><span class="text-slate-500">Discount Percent:</span> <span class="font-medium">${rawData.paymentTerms.discountPercent || '-'}%</span></div>
                            <div><span class="text-slate-500">Net Due Days:</span> <span class="font-medium">${rawData.paymentTerms.netDueDays || '-'}</span></div>
                        </div>
                    </div>
                </details>
                ` : ''}

                <details class="mt-4 border border-slate-200 rounded-lg">
                    <summary class="cursor-pointer p-3 bg-slate-50 font-bold text-slate-700 hover:bg-slate-100 rounded-t-lg">View Raw JSON Data</summary>
                    <pre class="p-4 bg-slate-900 text-slate-300 rounded-b-lg text-xs overflow-auto max-h-64">${JSON.stringify(rawData, null, 2)}</pre>
                </details>
            `;

            document.getElementById('poModal').classList.remove('hidden');
            document.getElementById('poModal').classList.add('flex');
        }

        function closePOModal() {
            document.getElementById('poModal').classList.add('hidden');
            document.getElementById('poModal').classList.remove('flex');
        }

        // Close modal on outside click
        document.getElementById('poModal').addEventListener('click', function(e) {
            if (e.target === this) closePOModal();
        });

        // Handle URL query parameter to scroll to specific PO
        document.addEventListener('DOMContentLoaded', function() {
            const urlParams = new URLSearchParams(window.location.search);
            const targetPO = urlParams.get('po');

            if (targetPO) {
                const row = document.getElementById('po-row-' + targetPO);
                if (row) {
                    // Highlight the row
                    row.classList.add('bg-blue-100', 'ring-2', 'ring-blue-500', 'ring-inset');
                    row.classList.remove('hover:bg-slate-50');

                    // Scroll to the row after a short delay
                    setTimeout(() => {
                        row.scrollIntoView({ behavior: 'smooth', block: 'center' });

                        // Auto-open the PO details modal
                        showPODetails(targetPO);
                    }, 200);

                    // Remove highlight after 5 seconds but keep the row visible
                    setTimeout(() => {
                        row.classList.remove('bg-blue-100', 'ring-2', 'ring-blue-500', 'ring-inset');
                        row.classList.add('hover:bg-slate-50', 'bg-yellow-50');
                    }, 5000);
                }
            }
        });
    </script>
</body>
</html>
