<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Purchase Orders - Amazon Tracker</title>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;900&display=swap" rel="stylesheet">
    <script src="https://cdn.tailwindcss.com"></script>
    <style>
        body { font-family: 'Inter', sans-serif; }
        .status-badge {
            display: inline-block;
            padding: 4px 10px;
            border-radius: 9999px;
            font-size: 0.7rem;
            font-weight: 700;
            text-transform: uppercase;
        }
        .status-new { background: #dbeafe; color: #1e40af; }
        .status-acknowledged { background: #fef3c7; color: #92400e; }
        .status-shipped { background: #d1fae5; color: #065f46; }
        .status-cancelled { background: #fee2e2; color: #991b1b; }
        .status-closed { background: #e5e7eb; color: #374151; }
        .loading-spinner {
            border: 3px solid #f3f3f3;
            border-top: 3px solid #3498db;
            border-radius: 50%;
            width: 24px;
            height: 24px;
            animation: spin 1s linear infinite;
        }
        @keyframes spin {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
        }
        .table-container { overflow-x: auto; }
        thead th { position: sticky; top: 0; z-index: 10; }
    </style>
</head>
<body class="bg-slate-100 min-h-screen">
    <div class="max-w-full mx-auto px-4 md:px-8 py-8">
        <!-- Header -->
        <header class="mb-8">
            <div class="flex flex-col md:flex-row justify-between items-start md:items-center gap-4">
                <div>
                    <h1 class="text-4xl font-black text-slate-900 tracking-tight">Purchase Orders</h1>
                    <p class="text-slate-500 font-medium">Vendor Central purchase orders from Amazon</p>
                </div>
                <div class="flex gap-3">
                    <a href="/" class="bg-slate-600 text-white px-6 py-3 rounded-xl font-bold shadow-lg hover:bg-slate-700 transition-all">Dashboard</a>
                    <a href="/vendor-analytics" class="bg-blue-600 text-white px-6 py-3 rounded-xl font-bold shadow-lg hover:bg-blue-700 transition-all">Vendor Analytics</a>
                </div>
            </div>
        </header>

        <!-- Date Filter & Sync Controls -->
        <div class="bg-white rounded-2xl shadow-lg p-6 mb-8 border border-slate-200">
            <div class="flex flex-wrap items-end gap-4">
                <!-- Calendar Date Range Picker -->
                <div class="flex flex-col w-full sm:w-auto relative">
                    <label class="text-xs font-bold text-slate-500 uppercase tracking-wider mb-1">Date Range Filter</label>
                    <button onclick="toggleDateRangePicker()" id="dateRangeBtn" class="px-5 py-2 rounded-xl border-2 border-slate-300 outline-none focus:ring-2 focus:ring-purple-500 text-sm bg-white text-left flex items-center gap-2 min-w-[220px] hover:border-purple-400 transition-all">
                        <span id="dateRangeLabel">Loading...</span>
                        <svg class="w-4 h-4 ml-auto" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"></path></svg>
                    </button>

                    <!-- Date Range Picker Dropdown -->
                    <div id="dateRangePicker" class="hidden fixed bg-white rounded-2xl shadow-2xl border border-slate-200 p-4 z-[99999] min-w-[340px]">
                        <!-- Quick Presets -->
                        <div class="flex flex-wrap gap-2 mb-4">
                            <button onclick="setDatePreset('all')" class="preset-btn px-3 py-1 rounded-lg text-xs font-bold bg-slate-100 hover:bg-purple-100 hover:text-purple-700 transition-all" data-preset="all">All POs</button>
                            <button onclick="setDatePreset('24h')" class="preset-btn px-3 py-1 rounded-lg text-xs font-bold bg-slate-100 hover:bg-purple-100 hover:text-purple-700 transition-all" data-preset="24h">24h</button>
                            <button onclick="setDatePreset('48h')" class="preset-btn px-3 py-1 rounded-lg text-xs font-bold bg-slate-100 hover:bg-purple-100 hover:text-purple-700 transition-all" data-preset="48h">48h</button>
                            <button onclick="setDatePreset('7d')" class="preset-btn px-3 py-1 rounded-lg text-xs font-bold bg-slate-100 hover:bg-purple-100 hover:text-purple-700 transition-all" data-preset="7d">7 Days</button>
                            <button onclick="setDatePreset('30d')" class="preset-btn px-3 py-1 rounded-lg text-xs font-bold bg-slate-100 hover:bg-purple-100 hover:text-purple-700 transition-all" data-preset="30d">30 Days</button>
                            <button onclick="setDatePreset('90d')" class="preset-btn px-3 py-1 rounded-lg text-xs font-bold bg-slate-100 hover:bg-purple-100 hover:text-purple-700 transition-all" data-preset="90d">90 Days</button>
                            <button onclick="showCustomCalendar(event)" class="preset-btn px-3 py-1 rounded-lg text-xs font-bold bg-slate-100 hover:bg-purple-100 hover:text-purple-700 transition-all" data-preset="custom">Custom Range</button>
                        </div>

                        <!-- Calendar Section (hidden by default) -->
                        <div id="calendarSection" class="hidden">
                            <div class="flex items-center justify-between mb-2">
                                <button onclick="changeCalendarMonth(-1); event.stopPropagation();" class="p-1 hover:bg-slate-100 rounded-lg"><svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7"></path></svg></button>
                                <span id="calendarMonthYear" class="font-bold text-slate-700"></span>
                                <button onclick="changeCalendarMonth(1); event.stopPropagation();" class="p-1 hover:bg-slate-100 rounded-lg"><svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path></svg></button>
                            </div>
                            <div class="grid grid-cols-7 gap-1 text-center text-xs mb-2">
                                <div class="font-bold text-slate-400 py-1">Su</div>
                                <div class="font-bold text-slate-400 py-1">Mo</div>
                                <div class="font-bold text-slate-400 py-1">Tu</div>
                                <div class="font-bold text-slate-400 py-1">We</div>
                                <div class="font-bold text-slate-400 py-1">Th</div>
                                <div class="font-bold text-slate-400 py-1">Fr</div>
                                <div class="font-bold text-slate-400 py-1">Sa</div>
                            </div>
                            <div id="calendarDays" class="grid grid-cols-7 gap-1 text-center text-sm"></div>
                            <div id="selectedRangeDisplay" class="mt-3 pt-3 border-t border-slate-200 text-xs text-slate-600 text-center">
                                Click to select start date, then end date
                            </div>
                            <div class="flex gap-2 mt-3">
                                <button onclick="clearDateRange(); event.stopPropagation();" class="flex-1 px-3 py-2 bg-slate-100 text-slate-700 font-bold rounded-lg hover:bg-slate-200 transition-all text-sm">Clear</button>
                                <button onclick="applyDateRangeAndClose(); event.stopPropagation();" class="flex-1 px-3 py-2 bg-purple-600 text-white font-bold rounded-lg hover:bg-purple-700 transition-all text-sm">Apply Filter</button>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="border-l border-slate-300 h-8 mx-2"></div>

                <!-- Sync Controls -->
                <div>
                    <label class="block text-xs font-bold text-slate-500 uppercase tracking-wider mb-1">Sync from API (Created After)</label>
                    <input type="date" id="createdAfter"
                           class="px-4 py-2 rounded-lg border border-slate-300 focus:ring-2 focus:ring-purple-500 focus:border-purple-500 outline-none">
                </div>
                <button onclick="syncPurchaseOrders()" id="syncBtn" class="bg-purple-600 text-white px-6 py-2 rounded-lg font-bold hover:bg-purple-700 transition-all flex items-center gap-2">
                    <span id="syncBtnText">Sync POs</span>
                    <div id="syncSpinner" class="loading-spinner hidden"></div>
                </button>
                <button onclick="syncReceivingStatus()" id="syncStatusBtn" class="bg-green-600 text-white px-6 py-2 rounded-lg font-bold hover:bg-green-700 transition-all flex items-center gap-2">
                    <span id="syncStatusBtnText">Sync Receiving</span>
                    <div id="syncStatusSpinner" class="loading-spinner hidden"></div>
                </button>
                <div id="syncStatus" class="text-sm text-slate-500 ml-4"></div>
            </div>
            <div class="mt-3 text-sm text-slate-500">
                Showing <%= purchaseOrders.length %> purchase orders from <%= startDate %> to <%= endDate %>
            </div>
        </div>

        <!-- Summary Cards -->
        <div class="grid grid-cols-2 md:grid-cols-5 gap-4 mb-8">
            <div class="bg-white rounded-xl shadow-lg p-4 border border-slate-200">
                <div class="text-2xl font-black text-slate-900" id="totalPOs"><%= purchaseOrders.length %></div>
                <div class="text-xs text-slate-500 font-medium">Total POs</div>
            </div>
            <div class="bg-blue-50 rounded-xl shadow-lg p-4 border border-blue-200">
                <div class="text-2xl font-black text-blue-700" id="newPOs"><%= purchaseOrders.filter(p => p.po_status === 'New').length %></div>
                <div class="text-xs text-blue-600 font-medium">New</div>
            </div>
            <div class="bg-amber-50 rounded-xl shadow-lg p-4 border border-amber-200">
                <div class="text-2xl font-black text-amber-700" id="acknowledgedPOs"><%= purchaseOrders.filter(p => p.po_status === 'Acknowledged').length %></div>
                <div class="text-xs text-amber-600 font-medium">Acknowledged</div>
            </div>
            <div class="bg-emerald-50 rounded-xl shadow-lg p-4 border border-emerald-200">
                <div class="text-2xl font-black text-emerald-700" id="shippedPOs"><%= purchaseOrders.filter(p => ['Shipped', 'Closed'].includes(p.po_status)).length %></div>
                <div class="text-xs text-emerald-600 font-medium">Shipped/Closed</div>
            </div>
            <div class="bg-red-50 rounded-xl shadow-lg p-4 border border-red-200">
                <div class="text-2xl font-black text-red-700" id="cancelledPOs"><%= purchaseOrders.filter(p => p.po_status === 'Cancelled').length %></div>
                <div class="text-xs text-red-600 font-medium">Cancelled</div>
            </div>
        </div>

        <!-- Purchase Orders Table -->
        <div class="bg-white rounded-2xl shadow-xl border border-slate-200 overflow-hidden">
            <div class="table-container max-h-[65vh] overflow-auto">
                <table class="w-full text-sm">
                    <thead class="bg-slate-900 text-white">
                        <tr>
                            <th class="px-4 py-3 text-left font-bold">PO Number</th>
                            <th class="px-4 py-3 text-left font-bold">Vendor Code</th>
                            <th class="px-4 py-3 text-left font-bold">Date</th>
                            <th class="px-4 py-3 text-center font-bold">Status</th>
                            <th class="px-4 py-3 text-left font-bold">Delivery Window</th>
                            <th class="px-4 py-3 text-center font-bold">Items</th>
                            <th class="px-4 py-3 text-left font-bold">Ship To</th>
                            <th class="px-4 py-3 text-center font-bold">Actions</th>
                        </tr>
                    </thead>
                    <tbody class="divide-y divide-slate-200">
                        <% if (purchaseOrders.length === 0) { %>
                        <tr>
                            <td colspan="8" class="px-4 py-12 text-center text-slate-500">
                                <p class="text-4xl mb-4">No Purchase Orders</p>
                                <p class="font-medium">No purchase orders synced yet</p>
                                <p class="text-sm">Click "Sync from SP-API" to fetch your vendor purchase orders</p>
                            </td>
                        </tr>
                        <% } else { %>
                        <% purchaseOrders.forEach(po => { %>
                        <tr id="po-row-<%= po.po_number %>" class="hover:bg-slate-50 transition-all duration-300">
                            <td class="px-4 py-3">
                                <span class="font-mono font-bold text-slate-800"><%= po.po_number %></span>
                            </td>
                            <td class="px-4 py-3 text-xs">
                                <%
                                const sellingParty = typeof po.selling_party === 'string' ? JSON.parse(po.selling_party) : po.selling_party;
                                const vendorCode = sellingParty?.partyId || '';
                                %>
                                <% if (vendorCode) { %>
                                <span class="font-mono font-semibold text-purple-700 bg-purple-50 px-2 py-1 rounded"><%= vendorCode %></span>
                                <% } else { %>
                                <span class="text-slate-400">-</span>
                                <% } %>
                            </td>
                            <td class="px-4 py-3 text-slate-600">
                                <% if (po.po_date) { %>
                                <%= new Date(po.po_date).toLocaleDateString('en-CA') %>
                                <% } else { %>
                                -
                                <% } %>
                            </td>
                            <td class="px-4 py-3 text-center">
                                <% const statusClass = {
                                    'New': 'status-new',
                                    'Acknowledged': 'status-acknowledged',
                                    'Shipped': 'status-shipped',
                                    'Closed': 'status-closed',
                                    'Cancelled': 'status-cancelled'
                                }[po.po_status] || 'status-new'; %>
                                <span class="status-badge <%= statusClass %>"><%= po.po_status || 'Unknown' %></span>
                            </td>
                            <td class="px-4 py-3 text-xs text-slate-600">
                                <% if (po.delivery_window_start && po.delivery_window_end) { %>
                                <div><%= new Date(po.delivery_window_start).toLocaleDateString('en-CA') %></div>
                                <div class="text-slate-400">to <%= new Date(po.delivery_window_end).toLocaleDateString('en-CA') %></div>
                                <% } else { %>
                                -
                                <% } %>
                            </td>
                            <td class="px-4 py-3 text-center">
                                <% const items = typeof po.items === 'string' ? JSON.parse(po.items) : po.items; %>
                                <% if (items && Array.isArray(items)) { %>
                                <span class="font-bold text-slate-800"><%= items.length %></span>
                                <span class="text-slate-500 text-xs">items</span>
                                <% } else { %>
                                -
                                <% } %>
                            </td>
                            <td class="px-4 py-3 text-xs">
                                <% const shipTo = typeof po.ship_to_party === 'string' ? JSON.parse(po.ship_to_party) : po.ship_to_party; %>
                                <% if (shipTo?.address) { %>
                                <div class="text-slate-700"><%= shipTo.address.city || '' %></div>
                                <div class="text-slate-400"><%= shipTo.address.stateOrRegion || '' %>, <%= shipTo.address.countryCode || '' %></div>
                                <% } else if (shipTo?.partyId) { %>
                                <div class="text-slate-700 font-medium"><%= shipTo.partyId %></div>
                                <div class="text-slate-400 text-[10px]">Amazon FC</div>
                                <% } else { %>
                                -
                                <% } %>
                            </td>
                            <td class="px-4 py-3 text-center">
                                <button onclick="showPODetails('<%= po.po_number %>')" class="text-purple-600 hover:text-purple-800 text-xs font-medium">
                                    View Details
                                </button>
                            </td>
                        </tr>
                        <% }); %>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>

        <!-- PO Details Modal -->
        <div id="poModal" class="fixed inset-0 bg-black bg-opacity-50 hidden items-center justify-center z-50">
            <div class="bg-white rounded-2xl p-8 max-w-4xl w-full mx-4 shadow-2xl max-h-[90vh] overflow-auto">
                <div class="flex justify-between items-center mb-6">
                    <h3 class="text-xl font-bold text-slate-900">Purchase Order Details</h3>
                    <button onclick="closePOModal()" class="text-slate-400 hover:text-slate-600 text-2xl">&times;</button>
                </div>
                <div id="poModalContent">
                    <!-- Content will be inserted here -->
                </div>
            </div>
        </div>
    </div>

    <script>
        const purchaseOrders = <%- JSON.stringify(purchaseOrders) %>;
        const productTitles = <%- JSON.stringify(productTitles || {}) %>;
        const vendorSkus = <%- JSON.stringify(vendorSkus || {}) %>;
        const inventoryByAsin = <%- JSON.stringify(inventoryByAsin || {}) %>;
        const receivingData = <%- JSON.stringify(receivingData || {}) %>;

        // ============================================
        // Date Range Picker Calendar for PO Filtering
        // ============================================
        let calendarDate = new Date();
        let rangeStartDate = null;
        let rangeEndDate = null;
        let currentPreset = '90d';

        const urlParams = new URLSearchParams(window.location.search);
        const urlStartDate = urlParams.get('startDate');
        const urlEndDate = urlParams.get('endDate');

        document.addEventListener('DOMContentLoaded', function() {
            document.getElementById('createdAfter').value = new Date(Date.now() - 30 * 24 * 60 * 60 * 1000).toISOString().split('T')[0];

            if (urlStartDate && urlEndDate) {
                const start = new Date(urlStartDate);
                const end = new Date(urlEndDate);
                const daysDiff = Math.round((end - start) / (1000 * 60 * 60 * 24));
                if (daysDiff === 0) currentPreset = '24h';
                else if (daysDiff === 1) currentPreset = '48h';
                else if (daysDiff >= 6 && daysDiff <= 7) currentPreset = '7d';
                else if (daysDiff >= 29 && daysDiff <= 31) currentPreset = '30d';
                else if (daysDiff >= 89 && daysDiff <= 91) currentPreset = '90d';
                else { currentPreset = 'custom'; rangeStartDate = start; rangeEndDate = end; }
            } else if (!urlStartDate && !urlEndDate) {
                currentPreset = 'all';
            }

            updateDateRangeLabel();
            updatePresetButtons();

            document.addEventListener('mousedown', function(e) {
                const picker = document.getElementById('dateRangePicker');
                const btn = document.getElementById('dateRangeBtn');
                if (picker && !picker.classList.contains('hidden') && !picker.contains(e.target) && !btn.contains(e.target)) {
                    picker.classList.add('hidden');
                }
            });
        });

        function toggleDateRangePicker() {
            const picker = document.getElementById('dateRangePicker');
            const btn = document.getElementById('dateRangeBtn');
            picker.classList.toggle('hidden');
            if (!picker.classList.contains('hidden')) {
                const rect = btn.getBoundingClientRect();
                picker.style.top = (rect.bottom + 8) + 'px';
                picker.style.left = rect.left + 'px';
                document.getElementById('calendarSection').classList.add('hidden');
                updatePresetButtons();
            }
        }

        function showCustomCalendar(event) {
            if (event) event.stopPropagation();
            document.getElementById('calendarSection').classList.remove('hidden');
            currentPreset = 'custom';
            renderCalendar();
        }

        function changeCalendarMonth(delta) {
            calendarDate.setMonth(calendarDate.getMonth() + delta);
            renderCalendar();
        }

        function renderCalendar() {
            const year = calendarDate.getFullYear();
            const month = calendarDate.getMonth();
            const monthNames = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
            document.getElementById('calendarMonthYear').textContent = `${monthNames[month]} ${year}`;

            const firstDay = new Date(year, month, 1).getDay();
            const daysInMonth = new Date(year, month + 1, 0).getDate();
            let daysHtml = '';

            for (let i = 0; i < firstDay; i++) daysHtml += '<div></div>';

            for (let day = 1; day <= daysInMonth; day++) {
                const date = new Date(year, month, day);
                const isStart = rangeStartDate && isSameDay(date, rangeStartDate);
                const isEnd = rangeEndDate && isSameDay(date, rangeEndDate);
                const isInRange = rangeStartDate && rangeEndDate && date > rangeStartDate && date < rangeEndDate;
                const isToday = isSameDay(date, new Date());

                let classes = 'py-1 px-2 rounded cursor-pointer hover:bg-purple-100 ';
                if (isStart || isEnd) classes += 'bg-purple-600 text-white hover:bg-purple-700 font-bold ';
                else if (isInRange) classes += 'bg-purple-200 ';
                else if (isToday) classes += 'border border-purple-400 ';

                daysHtml += `<div class="${classes}" onclick="selectDate(${day})">${day}</div>`;
            }

            document.getElementById('calendarDays').innerHTML = daysHtml;
            updateSelectedRangeDisplay();
        }

        function selectDate(day) {
            const date = new Date(calendarDate.getFullYear(), calendarDate.getMonth(), day);
            date.setHours(0, 0, 0, 0);

            if (!rangeStartDate || (rangeStartDate && rangeEndDate)) {
                rangeStartDate = date;
                rangeEndDate = null;
            } else {
                if (date < rangeStartDate) { rangeEndDate = rangeStartDate; rangeStartDate = date; }
                else rangeEndDate = date;
                rangeEndDate.setHours(23, 59, 59, 999);
                currentPreset = 'custom';
            }
            renderCalendar();
        }

        function setDatePreset(preset) {
            const now = new Date();
            currentPreset = preset;

            if (preset === 'all') { window.location.href = '/purchase-orders'; return; }

            switch(preset) {
                case '24h':
                    rangeStartDate = new Date(now.getFullYear(), now.getMonth(), now.getDate());
                    rangeEndDate = new Date(now); rangeEndDate.setHours(23, 59, 59, 999);
                    break;
                case '48h':
                    rangeStartDate = new Date(now); rangeStartDate.setDate(rangeStartDate.getDate() - 1);
                    rangeStartDate.setHours(0, 0, 0, 0);
                    rangeEndDate = new Date(now); rangeEndDate.setHours(23, 59, 59, 999);
                    break;
                case '7d':
                    rangeStartDate = new Date(now); rangeStartDate.setDate(rangeStartDate.getDate() - 7);
                    rangeStartDate.setHours(0, 0, 0, 0);
                    rangeEndDate = new Date(now); rangeEndDate.setHours(23, 59, 59, 999);
                    break;
                case '30d':
                    rangeStartDate = new Date(now); rangeStartDate.setDate(rangeStartDate.getDate() - 30);
                    rangeStartDate.setHours(0, 0, 0, 0);
                    rangeEndDate = new Date(now); rangeEndDate.setHours(23, 59, 59, 999);
                    break;
                case '90d':
                    rangeStartDate = new Date(now); rangeStartDate.setDate(rangeStartDate.getDate() - 90);
                    rangeStartDate.setHours(0, 0, 0, 0);
                    rangeEndDate = new Date(now); rangeEndDate.setHours(23, 59, 59, 999);
                    break;
            }
            document.getElementById('calendarSection').classList.add('hidden');
            applyDateRangeAndClose();
        }

        function updatePresetButtons() {
            document.querySelectorAll('.preset-btn').forEach(btn => {
                const preset = btn.getAttribute('data-preset');
                if (preset === currentPreset) {
                    btn.classList.add('bg-purple-600', 'text-white');
                    btn.classList.remove('bg-slate-100', 'hover:bg-purple-100', 'hover:text-purple-700');
                } else {
                    btn.classList.remove('bg-purple-600', 'text-white');
                    btn.classList.add('bg-slate-100', 'hover:bg-purple-100', 'hover:text-purple-700');
                }
            });
        }

        function isSameDay(d1, d2) { return d1.getFullYear() === d2.getFullYear() && d1.getMonth() === d2.getMonth() && d1.getDate() === d2.getDate(); }
        function formatDateShort(date) { return date.toLocaleDateString('en-US', { month: 'short', day: 'numeric', year: 'numeric' }); }
        function formatDateForUrl(date) { return date.toISOString().split('T')[0]; }

        function clearDateRange() { rangeStartDate = null; rangeEndDate = null; currentPreset = 'all'; renderCalendar(); updateDateRangeLabel(); }

        function applyDateRangeAndClose() {
            if (!rangeStartDate || !rangeEndDate) { alert('Please select both start and end dates'); return; }
            window.location.href = `/purchase-orders?startDate=${formatDateForUrl(rangeStartDate)}&endDate=${formatDateForUrl(rangeEndDate)}`;
        }

        function updateDateRangeLabel() {
            const label = document.getElementById('dateRangeLabel');
            if (currentPreset === 'all') label.textContent = 'All Purchase Orders';
            else if (currentPreset === '24h') label.textContent = 'Last 24 Hours';
            else if (currentPreset === '48h') label.textContent = 'Last 48 Hours';
            else if (currentPreset === '7d') label.textContent = 'Last 7 Days';
            else if (currentPreset === '30d') label.textContent = 'Last 30 Days';
            else if (currentPreset === '90d') label.textContent = 'Last 90 Days';
            else if (currentPreset === 'custom' && rangeStartDate && rangeEndDate) label.textContent = `${formatDateShort(rangeStartDate)} - ${formatDateShort(rangeEndDate)}`;
            else if (urlStartDate && urlEndDate) label.textContent = `${urlStartDate} to ${urlEndDate}`;
            updatePresetButtons();
        }

        function updateSelectedRangeDisplay() {
            const display = document.getElementById('selectedRangeDisplay');
            if (!rangeStartDate) display.textContent = 'Click to select start date, then end date';
            else if (!rangeEndDate) display.textContent = `Start: ${formatDateShort(rangeStartDate)} - Click to select end date`;
            else display.textContent = `${formatDateShort(rangeStartDate)} - ${formatDateShort(rangeEndDate)}`;
        }

        async function syncPurchaseOrders() {
            const createdAfter = document.getElementById('createdAfter').value;
            const createdBefore = document.getElementById('endDate').value; // Use page end date

            const syncBtn = document.getElementById('syncBtn');
            const syncBtnText = document.getElementById('syncBtnText');
            const syncSpinner = document.getElementById('syncSpinner');
            const syncStatus = document.getElementById('syncStatus');

            syncBtn.disabled = true;
            syncBtnText.textContent = 'Syncing...';
            syncSpinner.classList.remove('hidden');
            syncStatus.textContent = '';

            try {
                const response = await fetch('/api/purchase-orders/sync', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        createdAfter: createdAfter ? createdAfter + 'T00:00:00Z' : undefined,
                        createdBefore: createdBefore ? createdBefore + 'T23:59:59Z' : undefined
                    })
                });

                const data = await response.json();

                if (data.success) {
                    let message = `Synced ${data.savedCount} orders`;
                    if (data.pagesFetched > 1) {
                        message += ` (${data.pagesFetched} pages)`;
                    }
                    if (data.hasMore) {
                        message += ' - more available, sync again';
                    }
                    syncStatus.textContent = message;
                    syncStatus.className = 'text-sm text-green-600 ml-4';

                    // Refresh page after short delay
                    setTimeout(() => window.location.reload(), 1500);
                } else {
                    syncStatus.textContent = 'Sync failed: ' + (data.error || 'Unknown error');
                    syncStatus.className = 'text-sm text-red-600 ml-4';
                }
            } catch (err) {
                syncStatus.textContent = 'Error: ' + err.message;
                syncStatus.className = 'text-sm text-red-600 ml-4';
            } finally {
                syncBtn.disabled = false;
                syncBtnText.textContent = 'Sync POs';
                syncSpinner.classList.add('hidden');
            }
        }

        async function syncReceivingStatus() {
            const syncBtn = document.getElementById('syncStatusBtn');
            const syncBtnText = document.getElementById('syncStatusBtnText');
            const syncSpinner = document.getElementById('syncStatusSpinner');
            const syncStatus = document.getElementById('syncStatus');

            syncBtn.disabled = true;
            syncBtnText.textContent = 'Syncing...';
            syncSpinner.classList.remove('hidden');
            syncStatus.textContent = 'Fetching receiving status from Amazon...';
            syncStatus.className = 'text-sm text-blue-600 ml-4';

            try {
                // Get last 60 days of updates
                const updatedAfter = new Date(Date.now() - 60 * 24 * 60 * 60 * 1000).toISOString();

                const response = await fetch('/api/purchase-orders/sync-status', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ updatedAfter })
                });

                const data = await response.json();

                if (data.success) {
                    let message = `Updated ${data.itemsUpdated} items from ${data.ordersProcessed} orders`;
                    if (data.pagesFetched > 1) {
                        message += ` (${data.pagesFetched} pages)`;
                    }
                    syncStatus.textContent = message;
                    syncStatus.className = 'text-sm text-green-600 ml-4';

                    // Refresh page after short delay
                    setTimeout(() => window.location.reload(), 1500);
                } else {
                    syncStatus.textContent = 'Sync failed: ' + (data.error || 'Unknown error');
                    syncStatus.className = 'text-sm text-red-600 ml-4';
                }
            } catch (err) {
                syncStatus.textContent = 'Error: ' + err.message;
                syncStatus.className = 'text-sm text-red-600 ml-4';
            } finally {
                syncBtn.disabled = false;
                syncBtnText.textContent = 'Sync Receiving';
                syncSpinner.classList.add('hidden');
            }
        }

